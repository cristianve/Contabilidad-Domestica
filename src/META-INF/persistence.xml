<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
	version="1.0">
	<persistence-unit name="ProyectoORM" transaction-type="RESOURCE_LOCAL">
		<provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
		<class>com.codo.modelo.pojos.Cuentas</class>
		<class>com.codo.modelo.pojos.Etiquetas</class>
		<class>com.codo.modelo.pojos.Monedas</class>
		<class>com.codo.modelo.pojos.Movimientos</class>
		<class>com.codo.modelo.pojos.Previsiones</class>
		<class>com.codo.modelo.pojos.Tipos</class>
		<properties>
			<property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver" />
			<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost/db_contabilidad_domestica" />
			<property name="javax.persistence.jdbc.user" value="root" />
			<property name="javax.persistence.jdbc.password" value="Lenovo1" />
		</properties>
	</persistence-unit>
</persistence>